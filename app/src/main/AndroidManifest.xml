<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.russell.frogencyclopedia" android:installLocation="auto">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.russell.frogencyclopedia.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.russell.frogencyclopedia.SeeFrogs"></activity>
        <activity android:name="com.example.russell.frogencyclopedia.FrogInfo"></activity>
        <activity android:name="com.example.russell.frogencyclopedia.TriviaGame"></activity>
        <activity android:name="com.example.russell.frogencyclopedia.TriviaGameOver"></activity>
        <activity android:name="com.example.russell.frogencyclopedia.TriviaHelp"></activity>
        <activity android:name="com.example.russell.frogencyclopedia.TriviaPlay"></activity>
    </application>

</manifest>
